  <br>
    <div class="container">
        <ul class="month-display">
             {{#each this}}
            <li>
                <span class="sun">{{{this.summary}}}</span> 
                <span class="pretty-date">{{shortDate this.date}}</span><br>
                <span class="extra-summary">{{{from_index this.extra_information 0}}}</span>
            </li>
            {{/each}}
        </ul>
    </div>